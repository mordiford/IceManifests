<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
  <!-- Android-8.1.0_r26 (security patch level: 2018-05-05) -->
  <remove-project name="AICP/build" />
  <project path="build/make" name="mordiford/build" groups="pdk" remote="aicp" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <remove-project name="AICP/build_soong" />
  <project path="build/soong" name="mordiford/build_soong" groups="pdk,tradefed" remote="aicp" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <remove-project name="AICP/frameworks_av" />
  <project name="mordiford/frameworks_av" path="frameworks/av" remote="aicp" />

  <remove-project name="AICP/frameworks_native" />
  <project name="mordiford/frameworks_native" path="frameworks/native" remote="aicp" />

  <remove-project name="AICP/packages_services_Telephony" />
  <project name="mordiford/packages_services_Telephony" path="packages/services/Telephony" remote="aicp" />
  
  <remove-project name="AICP/system_core" />
  <project path="system/core" name="mordiford/system_core" groups="pdk" remote="aicp" />  
  
  <!-- mordiford-ify patch -->
  <remove-project name="AICP/frameworks_base" />
  <project name="mordiford/frameworks_base" path="frameworks/base" remote="aicp" />
  
  <remove-project name="AICP/lineage-sdk" />
  <project name="mordiford/lineage-sdk" path="lineage-sdk" remote="aicp" />

  <remove-project name="AICP/packages_apps_AicpExtras"/>
  <project name="mordiford/packages_apps_AicpExtras" path="packages/apps/AicpExtras" remote="aicp" />

  <remove-project name="AICP/packages_apps_DUI" />
  <project name="mordiford/packages_apps_DUI" path="packages/apps/DUI" remote="aicp" />

  <remove-project name="AICP/vendor_aicp"/>
  <project name="mordiford/vendor_aicp" path="vendor/aicp" remote="aicp" />

  <!-- clang 7.0.1 -->
  <remove-project name="platform/prebuilts/clang/host/linux-x86" />
  <project name="mordiford/prebuilts_clang" path="prebuilts/clang/host/linux-x86" clone-depth="1" remote="aicp" />
</manifest>
